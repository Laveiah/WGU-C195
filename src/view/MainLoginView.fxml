<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="407.0" prefWidth="585.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainLogInController">
   <children>
      <Label fx:id="loginTitle" layoutX="216.0" layoutY="39.0" text="Main Screen">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <TextField fx:id="userNameLogIn" alignment="CENTER" layoutX="220.0" layoutY="126.0" promptText="User Name" />
      <TextField fx:id="passwordLogIn" alignment="CENTER" layoutX="220.0" layoutY="188.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="220.0" layoutY="241.0" mnemonicParsing="false" onAction="#onLoginButton" text="Login" />
      <Button fx:id="loginExitButton" layoutX="332.0" layoutY="241.0" mnemonicParsing="false" onAction="#onLoginExitButton" prefHeight="26.0" prefWidth="48.0" text="Exit" />
      <Label fx:id="loginScreenLocationLabel" layoutX="264.0" layoutY="320.0" text="Location">
         <font>
            <Font size="16.0" />
         </font>
      </Label>

   </children>
</AnchorPane>
